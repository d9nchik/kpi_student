{
  "name": "frontend",
  "version": "1.0.0",
  "description": "frontend of kpi_student",
  "main": "index.js",
  "dependencies": {
    "dayjs": "^1.10.4",
    "firebase": "^8.3.2",
    "react-router-dom": "^5.2.0",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'  --html sme_result.html",
    "lint": "eslint \"src/**/*.{t,j}s{x,}\"",
    "lint:write": "eslint --debug src/ --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true npm run test:watch --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "npm run test:watch -- --coverage",
    "eject": "react-scripts eject",
    "emulator": "firebase --project='kpi-student' emulators:start --only auth,firestore,pubsub"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.20.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-sonarjs": "^0.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.2.4"
  },
  "keywords": [
    "student",
    "game",
    "frontend",
    "js",
    "react",
    "typescript",
    "ts",
    "javascript"
  ],
  "author": "d9nchik",
  "license": "MIT"
}
